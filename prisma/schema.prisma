// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Reminder {
  id Int @id

  userId         String
  content        String
  time           DateTime
  // time interval after which to repeat, in seconds
  repeatInterval Int
}
